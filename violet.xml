<?xml version="1.0" encoding="UTF-8"?>
<!--
Project:    violet gedit theme
Version:    0.1.0
-->
<style-scheme id="Vio" name="Violet" filename="violet.xml" version="1.0">
  <author>TheFallnn @ Github</author>
  <description>A swift Dark Rich violet theme</description>

  <color name="violet0" value="#1A1B26" />
  <color name="violet1" value="#1E1F29" />
  <color name="violet2" value="#736192" />
  <color name="violet3" value="#4a3e5d" />
  <color name="violet3-brightened" value="#6a5986" />
  <color name="violet4" value="#D8DEE9" />
  <color name="violet5" value="#E5E9F0" />
  <color name="violet6" value="#ECEFF4" />

  <color name="violet7" value="#8FBCBB" />
  <color name="violet8" value="#88C0D0" />
  <color name="violet9" value="#81A1C1" />
  <color name="violet10" value="#5E81AC" />

  <color name="violet11" value="#BF616A" />
  <color name="violet12" value="#D08770" />
  <color name="violet13" value="#EBCB8B" />
  <color name="violet14" value="#A3BE8C" />
  <color name="violet15" value="#B48EAD" />

<!--+- - - - - +
    + Settings +
    +- - - - - +-->
  <style name="background-pattern" background="violet1"/>
  <style name="bracket-match" foreground="violet4" background="violet10"/>
  <style name="bracket-mismatch" foreground="violet11" underline="true" />
  <style name="current-line" foreground="violet4" background="violet1" />
  <style name="cursor" foreground="violet4" />
  <style name="draw-spaces" foreground="violet3" />
  <style name="line-numbers" foreground="violet3" background="violet0" />
  <style name="search-match" foreground="violet4" background="violet10" />
  <style name="secondary-cursor" use-style="cursor" />
  <style name="selection" foreground="violet4" background="violet2" />
  <style name="selection-unfocused" foreground="violet4" background="violet9" />
  <style name="right-margin" foreground="violet3" />
  <style name="text" foreground="violet4" background="violet0" />

<!--+- - - - - - -+
    + Definitions +
    +- - - - - - -+-->
  <!--+- Customs -+-->
  <style name="def:annotation" foreground="violet12" />
  <style name="def:escape" foreground="violet12" />
  <style name="def:regexp" foreground="violet13" />
  <style name="def:symbol" foreground="violet6" />

  <!--+- Variables -+-->
  <style name="def:number" foreground="violet15" />
  <style name="def:string" foreground="violet14" />
  <style name="def:base-n-integer" use-style="def:number" />
  <style name="def:character" use-style="def:string" />
  <style name="def:complex" use-style="def:number" />
  <style name="def:decimal" use-style="def:number" />
  <style name="def:floating-point" use-style="def:number" />
  <style name="def:special-char" foreground="violet12" />
  <style name="def:variable" foreground="violet4" />

  <!--+- Constants -+-->
  <style name="def:boolean" foreground="violet9" />
  <style name="def:constant" foreground="violet4" />
  <style name="def:namespace" foreground="violet7" />
  <style name="def:special-constant" foreground="violet9" />

  <!--+- Comments -+-->
  <style name="def:comment" foreground="violet3-brightened" />
  <style name="def:doc-comment" use-style="def:comment" />
  <style name="def:doc-comment-element" foreground="violet9" />
  <style name="def:net-address-in-comment" foreground="violet3-brightened" underline="true" />
  <style name="def:note" use-style="def:comment" />

  <!--+- Keywords -+-->
  <style name="def:class" foreground="violet7" />
  <style name="def:keyword" foreground="violet9" />
  <style name="def:attribute" use-style="def:class" />
  <style name="def:builtin" use-style="def:keyword" />
  <style name="def:identifier" use-style="def:keyword" />
  <style name="def:preprocessor" foreground="violet10" />
  <style name="def:reserved" use-style="def:keyword" />
  <style name="def:shebang" foreground="violet10" />
  <style name="def:statement" use-style="def:keyword" />
  <style name="def:tag" use-style="def:keyword" />
  <style name="def:type" use-style="def:class" />

  <!--+- Logicals -+-->
  <style name="def:error" foreground="violet11" />
  <style name="def:function" foreground="violet8" />
  <style name="def:operator" foreground="violet9" />

  <!--+- Headings -+-->
  <style name="def:heading0" foreground="violet8" />
  <style name="def:heading1" use-style="def:heading0" />
  <style name="def:heading2" use-style="def:heading0" />
  <style name="def:heading3" use-style="def:heading0" />
  <style name="def:heading4" use-style="def:heading0" />
  <style name="def:heading5" use-style="def:heading0" />
  <style name="def:heading6" use-style="def:heading0" />

  <!--+- Formats -+-->
  <style name="def:underlined" underline="true" />
  <style name="def:warning" foreground="violet13" />

<!--+- - - - - -+
    + Languages +
    +- - - - - -+-->
  <style name="css:at-rules" use-style="def:annotation" />
  <style name="css:block" use-style="def:symbol" />
  <style name="css:color" use-style="def:number" />
